$("#total_score").html('<h1><%= "#{@deal.plusminus}" %></h1>')
$("div#user_like").html("<%= escape_javascript(render("deals/user_like")) %>")
$("div#user_dislike").html("<%= escape_javascript(render("deals/user_dislike")) %>")
$("span#point_count").html("<%= "#{@deal.point_count}" %>")

<% if @first_vote == "yes" %>
	<% if @current_level == current_user.level %>
		showPointsPopup(15);
	<% else %>
		showLeveledUp("<%= current_user.title %>");
	<% end %>
<% end %>